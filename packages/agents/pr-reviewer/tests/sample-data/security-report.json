{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "semgrep",
          "version": "1.45.0",
          "informationUri": "https://semgrep.dev"
        }
      },
      "results": [
        {
          "ruleId": "javascript.lang.security.hardcoded-secret.aws-secret-key",
          "level": "error",
          "message": {
            "text": "AWS secret key detected in source code"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/config/aws.ts"
                },
                "region": {
                  "startLine": 15,
                  "startColumn": 12,
                  "endLine": 15,
                  "endColumn": 28
                }
              }
            }
          ]
        },
        {
          "ruleId": "javascript.lang.security.audit.sql-injection",
          "level": "warning",
          "message": {
            "text": "Possible SQL injection vulnerability"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/database/queries.ts"
                },
                "region": {
                  "startLine": 42,
                  "startColumn": 8,
                  "endLine": 42,
                  "endColumn": 45
                }
              }
            }
          ]
        },
        {
          "ruleId": "javascript.lang.security.audit.xss",
          "level": "warning",
          "message": {
            "text": "Unsanitized user input may lead to XSS"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/web/render.ts"
                },
                "region": {
                  "startLine": 67,
                  "startColumn": 15,
                  "endLine": 67,
                  "endColumn": 28
                }
              }
            }
          ]
        }
      ]
    }
  ]
}